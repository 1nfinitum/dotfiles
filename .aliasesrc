function add_alias() {
    if [[ -z $1 || -z $2 || $# -gt 2 ]]; then
        echo usage:
        echo "\t\$$0 ll 'ls -l'"
    else
        echo "alias $1='$2'" >> $ALIASFILE
        echo "alias ADDED to $ALIASFILE"
    fi
}
function dex() {
  if [[ -z $1 || $# -gt 1  ]]; then
    echo usage:
    echo "$0 container_name"
  else
  docker exec -it $1 bash
  fi

}
function dcx() {
  if [[ -z $1 || $# -gt 1  ]]; then
    echo usage:
    echo "$0 docker-compose service name"
  else
  docker-compose exec $1 bash
  fi

}
